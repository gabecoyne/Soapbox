<%= semantic_form_for([:admin, @member]) do |form| %>

	<%= render "admin/shared/form_buttons", :form => form %>

	<%= render "admin/shared/form_errors" %>

	<div class="tabs">
		
		<ul>
			<li><a href="#tab_main">Main</a></li>
		</ul>		
	
		<div id="tab_main">
			<%= form.inputs do -%>
			  	<%= form.inputs do -%>
						<%= form.input :email, :disabled => true %>
				  <%- end -%>
	    <%- end -%>
		</div>
	
	</div>

	<%= render "admin/shared/form_buttons", :form => form %>

<% end %>